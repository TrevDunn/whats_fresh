<div class="main-section">
	<div class="main-heading">
		<h1> Log In </h1>
	</div>
	<%= form_tag '/login' do %>
		<div class="login-div">
		<ul>
			<li>
				Email: <%= text_field_tag :email%>
			</li>
			<li>
				Password: <%= password_field_tag :password  %>
			</li>
		</ul>
		<%= submit_tag "Submit" %>
		</div>
	<% end %>
</div>
